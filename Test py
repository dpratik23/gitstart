# Navigate to the xyz folder
Set-Location -Path "C:\path\to\xyz"

# Get all the files with .zip.ARCH extension
$files = Get-ChildItem -Filter "*.zip.ARCH"

# Rename each file
foreach ($file in $files) {
    $newName = [System.IO.Path]::ChangeExtension($file.FullName, ".zip")
    Rename-Item -Path $file.FullName -NewName $newName
}
